CLASS zcl_s4d400_20_oo DEFINITION  PUBLIC  FINAL  CREATE PUBLIC .    PUBLIC SECTION.      INTERFACES if_oo_adt_classrun .  PROTECTED SECTION.  PRIVATE SECTION.ENDCLASS.      CLASS zcl_s4d400_20_oo IMPLEMENTATION.      METHOD if_oo_adt_classrun~main.      DATA lr_vehicle TYPE REF TO lcl_vehicle.    lr_vehicle = new lcl_vehicle(        iv_make = 'Lexus'        iv_model = 'LFA'     ).      DATA lr_vehicle2 TYPE REF TO lcl_vehicle.    lr_vehicle2 = new lcl_vehicle(        iv_make = 'Ferrari'        iv_model = 'F355'     ).      DATA lr_vehicle3 TYPE REF TO lcl_vehicle.    lr_vehicle3 = new lcl_vehicle(        iv_make = 'Porsche'        iv_model = '993 Turbo'     ).  *    lr_vehicle->set_make( 'Lexus' ).*    lr_vehicle->set_model( 'LFA' ).*    lr_vehicle2->set_make( 'Ferrari' ).*    lr_vehicle2->set_model( 'F355' ).*    lr_vehicle3->set_make( 'Porsche' ).*    lr_vehicle3->set_model( '993 Turbo' ).      DATA lrt_vehicle TYPE TABLE OF REF TO lcl_vehicle.    APPEND lr_vehicle TO lrt_vehicle.    APPEND lr_vehicle2 TO lrt_vehicle.    APPEND lr_vehicle3 TO lrt_vehicle.      DATA lv_make_in_main_class TYPE string.    DATA lv_model_in_main_class TYPE string.      DATA lr_wa_vehicle TYPE REF TO lcl_vehicle.    LOOP AT lrt_vehicle INTO lr_wa_vehicle.  *    lr_wa_vehicle->hello_world(*        EXPORTING*            iv_import = |World|*        IMPORTING*            ev_make = lv_make_in_main_class*            ev_model = lv_model_in_main_class*     ).  *    lr_wa_vehicle->get_make(*        IMPORTING ev_make = lv_make_in_main_class ).*    lr_wa_vehicle->get_model(*        IMPORTING ev_model = lv_model_in_main_class ).      lr_wa_vehicle->get_make( receiving rv_make = lv_make_in_main_class ).    lr_wa_vehicle->get_model( receiving rv_model = lv_model_in_main_class ).      out->write( |Make: { lv_make_in_main_class } / Model: { lv_model_in_main_class }| ).      ENDLOOP.    ENDMETHOD.ENDCLASS.

